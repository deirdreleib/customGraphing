% Generated by roxygen2: do not edit by hand
% Please edit documentation in R/box_plot1.R
\name{box_plot1}
\alias{box_plot1}
\title{Box Plot 1}
\usage{
box_plot1(
  data,
  x,
  y,
  outline_color = "darkblue",
  fill = "lightgray",
  alpha = 0.7
)
}
\arguments{
\item{data}{Data frame}

\item{x}{x-axis variable}

\item{y}{y-axis variable}

\item{fill}{Box fill color}

\item{alpha}{Transparency of the fill color (0 to 1)}

\item{color}{Box outline color}
}
\value{
ggplot object
}
\description{
Box Plot 1
}
\examples{
# Example 1: Default box plot
data <- data.frame(
  category = rep(c("A", "B", "C"), each = 30),
  value = rnorm(90)
)
box_plot1(data, "category", "value")

# Example 2: Customized box plot with red outline and green fill
box_plot1(data, "category", "value", color = "red", fill = "green", alpha = 0.5)

}
