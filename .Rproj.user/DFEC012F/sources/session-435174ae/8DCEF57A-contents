% Generated by roxygen2: do not edit by hand
% Please edit documentation in R/graphFunctions.R
\name{customScatterPlot}
\alias{customScatterPlot}
\title{Custom Scatter Plot}
\usage{
customScatterPlot(x, y, color = "blue", size = 3)
}
\arguments{
\item{x}{Numeric vector for x-axis}

\item{y}{Numeric vector for y-axis}

\item{color}{Character, color of the points}

\item{size}{Numeric, size of the points}
}
\value{
A ggplot2 scatter plot
}
\description{
Custom Scatter Plot
}
\examples{
test_that("customScatterPlot generates a plot", {
  data <- data.frame(x = 1:10, y = rnorm(10))
  p <- customScatterPlot(data$x, data$y, color = "blue", size = 3)
  expect_true(inherits(p, "gg"))
})

}
