\name{customBarPlot}
\alias{customBarPlot}
\title{Custom Bar Plot}
\usage{
customBarPlot(x, y, fill = "steelblue")
}
\arguments{
\item{x}{Factor or character vector for x-axis}

\item{y}{Numeric vector for y-axis}

\item{fill}{Character, fill color of the bars}
}
\value{
A ggplot2 bar plot
}
\description{
Custom Bar Plot
}
\examples{
customBarPlot(x = LETTERS[1:5], y = c(3, 7, 2, 5, 9), fill = "green")


}
